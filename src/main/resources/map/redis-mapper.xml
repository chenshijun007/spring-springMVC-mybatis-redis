<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hanboard.dao.RedisDao">
    <select id="findUserName" resultType="String">
         UPDATE  user SET username="王麻子" where id ='2'
    </select>

    <select id="findAllCity" resultType="com.hanboard.model.City">
     SELECT
       City.id as id,
       City.cityName as cityName,
       City.cityNum as cityNum
       FROM City1
    </select>
</mapper>
